---
import Table from "../components/Table.astro";

const rows = await fetch("http://localhost:8080/api/events")
	.then((res) => res.json())
	.then((data) => data.data);

const cols = [
	{ name: "Name", key: "Name" },
	{ name: "Date", key: "CreatedAt" },
];
console.log(rows);
---

<h1>Events</h1>
<div class="m-5">
	<Table cols={cols} rows={rows} />
</div>
